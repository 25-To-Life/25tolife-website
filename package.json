		"@smui/icon-button": "^6.0.0",
		"@smui/list": "^6.0.0",
		"@smui/switch": "^6.0.0",
		"@smui/top-app-bar": "^6.0.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-node": "^1.0.0-next.83",
		"@sveltejs/kit": "next",
		"autoprefixer": "^10.4.7",
		"eslint": "^8.16.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.14",
		"prisma": "^4.1.0",
		"smui-theme": "^6.0.0",
		"svelte": "^3.44.0",
		"svelte-carousel": "^1.0.19",
